{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:application": {
      "strict": true
    },
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/uts-dpm-frontend/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/uts-dpm-frontend",
        "index": "apps/uts-dpm-frontend/src/index.html",
        "main": "apps/uts-dpm-frontend/src/main.ts",
        "polyfills": "apps/uts-dpm-frontend/src/polyfills.ts",
        "tsConfig": "apps/uts-dpm-frontend/tsconfig.app.json",
        "assets": [
          "apps/uts-dpm-frontend/src/favicon.ico",
          "apps/uts-dpm-frontend/src/assets"
        ],
        "styles": [
          "apps/uts-dpm-frontend/src/styles.scss",
          "node_modules/flatpickr/dist/flatpickr.css",
          "node_modules/primeng/resources/primeng.min.css",
          "node_modules/primeicons/primeicons.css",
          "node_modules/ngx-toastr/toastr.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/uts-dpm-frontend/src/environments/environment.ts",
              "with": "apps/uts-dpm-frontend/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "uts-dpm-frontend:build:production"
        },
        "development": {
          "browserTarget": "uts-dpm-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "uts-dpm-frontend:build"
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "apps/uts-dpm-frontend/src/test.ts",
        "polyfills": "apps/uts-dpm-frontend/src/polyfills.ts",
        "tsConfig": "apps/uts-dpm-frontend/tsconfig.spec.json",
        "karmaConfig": "apps/uts-dpm-frontend/karma.conf.js",
        "assets": [
          "apps/uts-dpm-frontend/src/favicon.ico",
          "apps/uts-dpm-frontend/src/assets"
        ],
        "styles": ["apps/uts-dpm-frontend/src/styles.scss"],
        "scripts": []
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/uts-dpm-frontend/**/*.ts",
          "apps/uts-dpm-frontend/**/*.html"
        ]
      }
    }
  }
}
